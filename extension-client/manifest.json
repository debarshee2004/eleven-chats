{
  "manifest_version": 3,
  "name": "Eleven-Chats Chrome Extension",
  "version": "0.0.1",
  "description": "A Chrome extension for AI-Chats with contextual awareness.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content.tsx"]
    }
  ]
}
